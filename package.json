{
  "name": "if-connection",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"nuxt\" \"yarn functions:watch\" \"yarn emulator\"",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "lintfix": "yarn lint:js --fix && yarn lint:style --fix",
    "download-firebase-config": "dotenv -- cross-var firebase apps:sdkconfig web %APP_ID% --json > firebase-config.json -P %PROJECT_ID%",
    "test": "jest",
    "functions:watch": "cd functions && yarn build --watch",
    "emulator": "firebase emulators:start --import emulator --export-on-exit emulator --only firestore,storage,functions,auth"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/firebase": "^8.2.2",
    "@nuxtjs/pwa": "^3.3.5",
    "@types/js-cookie": "^3.0.1",
    "algoliasearch": "^4.13.0",
    "cookieparser": "^0.1.0",
    "core-js": "^3.19.3",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^5.0.0",
    "firebase": "^9.6.9",
    "firebase-admin": "^10.0.2",
    "firebase-config": "^0.0.2",
    "jquery": "^3.6.1",
    "js-cookie": "^3.0.1",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vue-the-mask": "^0.11.1",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.6.4",
    "vuetify-js-utils": "^0.5.10",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/router": "^1.7.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@types/lodash": "^4.14.180",
    "@types/uuid": "^8.3.4",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.4.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.4.1",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "firebase-tools": "^10.5.0",
    "jest": "^27.4.4",
    "postcss-html": "^1.3.0",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "ts-jest": "^27.1.1",
    "vue-jest": "^3.0.4",
    "vuex-module-decorators": "^2.0.0"
  }
}
